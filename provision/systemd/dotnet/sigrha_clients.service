[Service]
Type=forking
User=chibi
Group=chibi

WorkingDirectory=/home/chibi/projects/clients_service/API_Clients/bin/Release/netcoreapp3.1/
ExecStart=/bin/dotnet /home/chibi/projects/clients_service/API_Clients/bin/Release/netcoreapp3.1/API_Clients.dll

Restart=always
RestartSec=10

KillSignal=SIGINT
SyslogIdentifier=Sigrha_client_service
EnvironmentFile=/etc/systemd/system/sigrha_clients.env

[Install]
WantedBy=multi-user.target
